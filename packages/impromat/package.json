  "resolutions": {
    "nth-check": "2.0.1"
  },
  "scripts": {
    "start": "yarn compile-markdown && react-scripts start",
    "dev": "yarn start",
    "build": "yarn compile-markdown && genversion src/version.gen.ts --es6 && react-scripts build",
    "build:serve": "serve build",
    "test": "yarn playwright:install && playwright test",
    "test:headed": "yarn test --headed",
    "test:watch": "nodemon --config nodemon-test.json",
    "test:debug": "yarn test:headed --debug",
    "playwright:codegen": "playwright codegen",
    "playwright:install": "playwright install --with-deps",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "compile-markdown": "bash ./scripts/generate-markdown.sh",
    "codegen:get-schema-json": "mkdir -p schema && cp ../impromat-api/schema/schema.gen.json schema/schema.gen.json",
    "codegen:schema-types": "json2ts -i schema/schema.gen.json -o src/store/schema.gen.ts",
    "codegen:schema-json-ts": "echo '/* AUTOGENERATED with codegen:schema-json-ts */\nexport const WorkshopSchemaJson = '$(cat schema/schema.gen.json) > src/store/schema-json.gen.ts ",
    "codegen": "yarn codegen:get-schema-json && yarn codegen:schema-types && yarn codegen:schema-json-ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "4.1.0",
    "@types/debug": "^4.1.7",
    "genversion": "^3.1.1",
    "gh-pages": "^4.0.0",
    "json-schema-to-typescript": "^11.0.2",
    "xo": "^0.51.0"
  },
  "description": "An Ionic project"
}
