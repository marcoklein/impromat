  "scripts": {
    "start": "yarn nx start",
    "dev": "yarn nx dev",
    "lint": "yarn nx lint",
    "test": "yarn nx test",
    "build": "yarn nx build",
    "nx": "nx",
    "test:unit:ui": "yarn nx test:unit --ui",
    "test:unit:update-snapshots": "yarn nx test:unit --update-snapshots",
    "test:e2e": "yarn playwright install && playwright test --config playwright-e2e.config.ts",
