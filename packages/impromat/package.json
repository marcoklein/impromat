{
  "name": "impromat",
  "version": "0.16.1",
  "private": true,
  "homepage": "https://impromat.app",
  "dependencies": {
    "@capacitor/app": "4.0.1",
    "@capacitor/core": "4.1.0",
    "@capacitor/haptics": "4.0.1",
    "@capacitor/keyboard": "4.0.1",
    "@capacitor/status-bar": "4.0.1",
    "@ionic/react": "^6.0.0",
    "@ionic/react-router": "^6.0.0",
    "@playwright/test": "^1.26.1",
    "@types/node": "^12.19.15",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.4",
    "debug": "^4.3.4",
    "flag-icons": "^6.6.6",
    "fuse.js": "^6.6.2",
    "graphql": "^16.6.0",
    "immer": "^9.0.15",
    "ionicons": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-markdown": "^8.0.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "rxdb": "^13.11.1",
    "rxdb-hooks": "^4.0.3",
    "rxjs": "^7.5.6",
    "serve": "^14.0.1",
    "typescript": "^4.1.3",
    "uuid": "^9.0.0",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "resolutions": {
    "nth-check": "2.0.1"
  },
  "scripts": {
    "start": "yarn compile-markdown && react-scripts start",
    "lint": "eslint src test",
    "lint:fix": "eslint src --fix",
    "dev": "yarn start",
    "build": "yarn compile-markdown && genversion src/version.gen.ts --es6 && react-scripts build",
    "build:serve": "serve --config serve.config.json build",
    "test": "yarn playwright:install && playwright test",
    "test:headed": "yarn test --headed",
    "test:watch": "nodemon --config nodemon-test.json",
    "test:debug": "yarn test:headed --debug",
    "playwright:codegen": "playwright codegen",
    "playwright:install": "playwright install chromium",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build",
    "compile-markdown": "bash ./scripts/generate-markdown.sh",
    "codegen:graphql": "yarn codegen:get-schema-graphql && graphql-codegen",
    "codegen:get-schema-graphql": "mkdir -p schema && cp ../impromat-api/schema/schema.graphql schema/schema.graphql",
    "codegen": "yarn codegen:get-schema-json && yarn codegen:graphql",
    "changeset": "yarn --cwd \"../..\" changeset"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "**/*.gen.ts"
    ],
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@capacitor/cli": "4.1.0",
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@types/debug": "^4.1.7",
    "eslint": "^8.30.0",
    "genversion": "^3.1.1",
    "gh-pages": "^4.0.0",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6"
  },
  "description": "An Ionic project"
}
