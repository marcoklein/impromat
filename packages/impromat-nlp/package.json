  },
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "natural": "^6.10.4",
    "stopword": "^2.0.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@types/stopword": "^2.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "yarn@3.4.1"
}
